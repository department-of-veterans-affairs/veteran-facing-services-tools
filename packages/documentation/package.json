    "**/axios": "^0.21.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "if [ -z \"$GITHUB_API_KEY\" ]; then echo \"No API key, skipping doc build\"; else GATSBY_CPU_COUNT=2 NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production gatsby build --prefix-paths; fi",
    "deploy": "gh-pages -d public -e .",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.jsx\" \"src/**/*.scss\" \"src/**/*.css\"",
    "lint:mdx": "remark \"src/**/*.mdx\" --quiet --frail",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "develop:inspect": "node --inspect --no-lazy node_modules/gatsby/dist/bin/gatsby develop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/department-of-veterans-affairs/veteran-facing-services-tools/"
  },
  "bugs": {
    "url": "https://github.com/department-of-veterans-affairs/veteran-facing-services-tools/"
  },
  "devDependencies": {
    "playwright": "^1.40.0",
    "remark-cli": "^12.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.1.0",
