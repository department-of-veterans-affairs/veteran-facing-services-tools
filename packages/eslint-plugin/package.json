{
  "name": "@department-of-veterans-affairs/eslint-plugin",
  "version": "1.2.3",
  "description": "ESLint plugin for va.gov projects",
  "homepage": "https://github.com/department-of-veterans-affairs/veteran-facing-services-tools/tree/master/packages/eslint-plugin#readme",
  "bugs": {
    "url": "https://github.com/department-of-veterans-affairs/veteran-facing-services-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:department-of-veterans-affairs/veteran-facing-services-tools.git"
  },
  "license": "MIT",
  "author": "Department of Veteran Affairs",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "lint": "eslint . --cache",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1"
  },
  "peerDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unicorn": "^35.0.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/helpers/"
    ]
  }
}
