{
  "src_folders": ["test/e2e"],
  "custom_commands_path": "test/commands",
  "custom_assertions_path": "test/assertions",
  "globals_path": "test/globals",
  "webdriver": {
    "start_process": true,
    "server_path": "node_modules/.bin/chromedriver",
    "port": 9515,
    "log_path": "logs"
  },

  "test_settings": {
    "default": {
      "webdriver": {
        "cli_args": ["debug"]
      },
      "desiredCapabilities": {
        "browserName": "chrome",
        "chromeOptions": {
          "args": ["--headless"]
        }
      }
    }
  }
}
